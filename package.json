{
  "name": "rucogs.github.io-backend",
  "version": "1.0.0",
  "scripts": {
    "start": "node dist/server",
    "start:recompile": "npm run compile && node dist/server",
    "start:rebuild": "npm -s run build && node dist/server",
    "generate": "typetta generate --config typetta.yml",
    "compile": "tsc && resolve-tspaths",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run generate && npm run compile"
  },
  "devDependencies": {
    "@types/passport": "^1.0.7",
    "@types/passport-discord": "^0.1.5",
    "@types/passport-google-oauth20": "^2.0.11",
    "resolve-tspaths": "^0.4.0",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "@twinlogix/typetta": "^1.6.4-2",
    "apollo-server": "^3.7.0",
    "dotenv": "^16.0.1",
    "passport": "^0.6.0",
    "passport-discord": "^0.1.4",
    "passport-google-oauth20": "^2.0.0",
    "rimraf": "^3.0.2"
  }
}
