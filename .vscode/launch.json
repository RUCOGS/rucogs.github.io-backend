{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Debug Production",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/server.js",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "env": { "NODE_ENV": "development" },
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "preLaunchTask": "prepare production debug",
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Debug Mock",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/_tools/mock-server.js",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "env": { "NODE_ENV": "development" },
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "preLaunchTask": "prepare mock debug",
    }
  ]
}